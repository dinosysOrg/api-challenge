<p id="notice"><%= notice %></p>

<h1>Listing of Total Expense by Categories</h1>

<table>
  <thead>
    <tr>
      <th>Category Name</th>
      <th>Total expense</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @expense_categories.each do |category| %>
      <tr>
        <td><%= category["name"] %></td>
        <td><%= category["total_expense"] %></td>
      </tr>
    <% end %>
  </tbody>
</table>
