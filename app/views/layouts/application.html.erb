<!DOCTYPE html>
<html>
  <head>
    <title>Dinosys</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>

    <%= stylesheet_link_tag "https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" %>
    <%= stylesheet_link_tag 'https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css' %>

    <%= stylesheet_link_tag "http://cdn.bootcss.com/animate.css/3.5.1/animate.min.css" %>

    <%= javascript_include_tag "https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" %>

    <%= stylesheet_link_tag "https://cdn.datatables.net/v/bs/dt-1.10.16/datatables.min.css" %>

    <%= javascript_include_tag "https://cdn.datatables.net/v/bs/dt-1.10.16/datatables.min.js" %>

    <%=  javascript_include_tag 'alert2.js' %>

    <%=  javascript_include_tag 'editable.js' %>

    <%=  stylesheet_link_tag 'editable.css' %>
    
  </head>

  <body>
    <br>  

    <nav class="navbar navbar-inverse">
      <div class="container-fluid">
        <div class="navbar-header">
          <a class="navbar-brand">Dinosys</a>
        </div>
        <ul class="nav navbar-nav">
          <li><%= link_to "Tournaments", tournaments_path , onclick: "window.location.reload();" %></li>
          <li><%= link_to "Groups", groups_path , onclick: "window.location.reload();" %></li>
          <li><%= link_to "Players", players_path , onclick: "window.location.reload();" %></li>
          <li><%= link_to "Venues", venues_path , onclick: "window.location.reload();" %></li>
          <li><%= link_to "Matches", matches_path , onclick: "window.location.reload();" %></li>
        </ul>
      </div>
    </nav>
    <%= yield %>
  </body>
</html>
